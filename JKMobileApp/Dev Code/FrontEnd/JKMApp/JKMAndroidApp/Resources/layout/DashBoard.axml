<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_gravity="center"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout1">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout1"
            android:layout_centerInParent="true"
            android:gravity="center_horizontal">
            <TextView
                android:text="Welcome to DashBoard !"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:textColor="@android:color/holo_blue_dark" />
        </LinearLayout>
      <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignParentBottom="true"
       android:orientation="horizontal"
       >
        <Button
            android:src="@drawable/account"
            android:background="#EC3D62"
            android:scaleType="centerInside"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:text="Contactus"
            android:layout_weight="1"
            android:theme="@style/ButtonTextAllCaps"
            android:textSize="@dimen/btn_text_size"
            android:textColor="@android:color/background_light"
            android:id="@+id/btncontactus" />
      </LinearLayout>

      <FrameLayout
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:id="@+id/framlayInfo"
     android:layout_below="@+id/relativeParent">
        <RelativeLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#CC000000"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/relativeClose">
          <LinearLayout
              android:orientation="vertical"
              android:minWidth="25px"
              android:minHeight="25px"
              android:id="@+id/linearLayout1"
              android:layout_marginTop="10dp"
              android:layout_marginLeft="10dp"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentLeft="true"
              android:layout_alignParentTop="true">
            <ImageView
                android:src="@drawable/icon_top_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
            />
            <ImageView
                android:src="@drawable/icon_top_left_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
            />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:clickable="false"
                android:textSize="9dp"   
                android:textColor="@android:color/background_light"
                android:text="Need more help ?" />
          </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:minWidth="25px"
              android:minHeight="25px"
              android:id="@+id/linearLayout2"
              android:layout_marginTop="10dp"
              android:layout_marginRight="10dp"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentRight="true"
              android:layout_alignParentTop="true">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
              <LinearLayout
                  android:orientation="horizontal"
                  android:minWidth="25px"
                  android:minHeight="25px"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content">
                <ImageView
                    android:src="@drawable/icon_top_right_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                 />
                <ImageView
                    android:src="@drawable/icon_top_noti"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                />
              </LinearLayout>
              <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_gravity="left"
                  android:clickable="false"
                  android:textSize="9dp"   
                  android:textColor="@android:color/background_light"
                  android:text="notification" />
            </LinearLayout>
          </LinearLayout>
          <LinearLayout
              android:orientation="horizontal"
              android:minWidth="25px"
              android:minHeight="25px"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_centerInParent="true">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:textSize="15sp"
                android:textColor="@android:color/background_light"
                android:text="Tap anywhere to " />
            <LinearLayout
             android:orientation="vertical"
             android:minWidth="25px"
             android:minHeight="25px"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content">
              <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:clickable="true"
                   android:textSize="15sp"
                   android:textColor="#db021b"
                   android:text="Close" />
              <View
                  android:layout_width="40dp"
                  android:layout_height="2dp"
                  android:background="#db021b"/>
            </LinearLayout>

          </LinearLayout>
          <LinearLayout
              android:orientation="horizontal"
              android:minWidth="25px"
              android:minHeight="25px"
              android:gravity="center"
              android:layout_weight="2"
              android:layout_marginBottom="10dp"
              android:layout_marginLeft="10dp"
              android:layout_marginRight="10dp"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_alignParentBottom="true">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_weight="2"
                android:layout_marginLeft="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right">
              <LinearLayout
                   android:orientation="horizontal"
                   android:layout_marginLeft="0dp"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:gravity="right">
                <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:clickable="false"
                   android:textSize="9dp"
                   android:textColor="@android:color/background_light"
                   android:text="you can view document here"
                   android:lines="2"
                   android:gravity="center_horizontal" />
              </LinearLayout>
              <LinearLayout
                  android:orientation="horizontal"
                  android:minWidth="25px"
                  android:minHeight="25px"
                  android:gravity="bottom"
                  
                  android:paddingRight="15dp"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                  <ImageView
                      android:src="@drawable/icon_bottom_left_arrow"
                      android:layout_width="60.0dp"
                      android:layout_height="138.0dp"
                      android:id="@+id/imageViewLeft2"
                      android:layout_marginBottom="0.0dp"
                      android:paddingBottom="15dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                  <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left">
                    <ImageView
                        android:src="@drawable/icon_doc"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:id="@+id/imageViewLeft1"
                        android:layout_marginTop="1.0dp" />
                  </LinearLayout>

                </LinearLayout>
              </LinearLayout>
              <LinearLayout
                  android:orientation="vertical"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:gravity="right">
                <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:clickable="false"
                   android:textSize="9dp"
                   android:textColor="@android:color/background_light"
                   android:text="MY DOCUMENTS"
                   android:gravity="center_horizontal" />
              </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"                
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">
              <TextView
                  android:layout_width="90dp"
                  android:layout_height="wrap_content"
                  android:clickable="false"
                  android:textSize="9dp"              
                  android:textColor="@android:color/background_light"
                  android:text="you can make\r\n payment here"
                  android:gravity="center_horizontal" />
              <LinearLayout
                  android:orientation="horizontal"
                  android:minWidth="25px"
                  android:minHeight="25px"
                  android:layout_marginLeft="5dp"
                  android:gravity="bottom"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content">
                <ImageView
                    android:src="@drawable/icon_bottom_center_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="88dp" />              
              </LinearLayout>
              <LinearLayout
                  android:orientation="vertical"
                  android:minWidth="25px"
                  android:minHeight="25px"
                  android:gravity="bottom"                
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content">
                <ImageView
                    android:src="@drawable/icon_pay"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:gravity="center_horizontal"
                    android:id="@+id/imageViewLeft1" />
              </LinearLayout>
              <LinearLayout
                 android:orientation="vertical"
                 android:minWidth="25px"
                 android:minHeight="25px"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content">
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:textSize="9dp"
                        android:textColor="@android:color/background_light"
                        android:text="PAYMENT"
                        android:gravity="center_horizontal" />
              </LinearLayout>
            </LinearLayout>
            <LinearLayout
           android:orientation="vertical"
           android:minWidth="25px"
           android:minHeight="25px"
           android:layout_weight="2"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:gravity="left"
           android:layout_marginBottom="0dp">
              <LinearLayout
                   android:orientation="vertical"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"                
                   android:gravity="center_horizontal"                  
                  >
              <TextView
                  android:layout_width="138.5dp"
                  android:layout_height="wrap_content"
                  android:clickable="false"
                  android:textSize="9dp"   
                  android:textColor="@android:color/background_light"
                  android:gravity="center_horizontal"
                  android:text="you can get your\r\n move team details\r\n here" />
               </LinearLayout>
              <LinearLayout
                  android:orientation="horizontal"
                  android:minWidth="25px"
                  android:minHeight="25px"
                  android:gravity="bottom"
                  android:layout_marginLeft="10dp"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginTop="5dp">
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:gravity="bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                  <ImageView
                      android:src="@drawable/icon_user"
                      android:layout_width="25dp"
                      android:layout_height="25dp" />                
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:gravity="right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                  <ImageView
                      android:src="@drawable/icon_bottom_right_arrow"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/imageViewLeft2"
                      android:layout_marginBottom="15dp" />
                </LinearLayout>
              </LinearLayout>
              <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left">
                <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:clickable="false"
                   android:textSize="9dp"
                   android:textColor="@android:color/background_light"
                   android:text="MY TEAM"
                   android:gravity="left" />
              </LinearLayout>
            </LinearLayout>
          </LinearLayout>
        </RelativeLayout>
      </FrameLayout>
    </RelativeLayout>
</LinearLayout>