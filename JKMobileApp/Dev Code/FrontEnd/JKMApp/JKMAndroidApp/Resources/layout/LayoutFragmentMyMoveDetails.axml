<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="9">
   <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_alignParentBottom="true"
      android:orientation="vertical"
      android:layout_weight="3">
         <LinearLayout
          android:layout_width="match_parent"
          android:orientation="horizontal"
          android:layout_marginLeft="20dp"
          android:layout_marginRight="20dp"
          android:layout_height="wrap_content"
          android:background="@drawable/background_shadow"
          android:layout_weight="1">
           <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="left"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="15dp"
            android:orientation="horizontal"
            android:layout_weight="2.5">
             <ImageView
             android:src="@drawable/icon_move_pack_date"
             android:layout_gravity="center"
             android:scaleType="centerInside"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"/>
           </LinearLayout>
           <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:gravity="center|right"
           android:layout_marginTop="20dp"
           android:orientation="vertical"
           android:layout_marginRight="20dp"
           android:layout_weight="0.5">
              <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_marginBottom="10dp"
            android:layout_gravity="center|right"
            android:textSize="@dimen/twelve_dp"
            android:textColor="#9FA6AD"
            android:id="@+id/tvPackdate"
            android:text="Pack Date" />
            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_gravity="center|right"
            android:textSize="@dimen/tv_text_size"
            android:textColor="@color/dark_gray"
            android:id="@+id/tvDisplayPackDate"
             />
           </LinearLayout>
         </LinearLayout>
         <LinearLayout
          android:layout_width="match_parent"
          android:orientation="vertical"
          android:layout_height="wrap_content">
           <ImageView
             android:src="@drawable/icon_arrow_pink"
             android:id="@+id/imgArrowpink"
             android:layout_gravity="center"
             android:scaleType="centerInside"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_weight="1" />
         </LinearLayout>
  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_alignParentBottom="true"
      android:orientation="vertical"
      android:layout_weight="3">
         <LinearLayout
          android:layout_width="match_parent"
          android:orientation="horizontal"
          android:layout_marginLeft="20dp"
          android:layout_marginRight="20dp"
          android:layout_height="wrap_content"
          android:background="@drawable/background_shadow"
          android:layout_weight="1">
           <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="left"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="15dp"
            android:orientation="horizontal"
            android:layout_weight="2.5">
             <ImageView
             android:src="@drawable/icon_move_load_date"
             android:layout_gravity="center"
             android:scaleType="centerInside"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"/>
           </LinearLayout>
           <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:gravity="center|right"
           android:layout_marginTop="20dp"
           android:orientation="vertical"
           android:layout_marginRight="20dp"
           android:layout_weight="0.5">
              <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_gravity="center|right"
            android:textSize="@dimen/twelve_dp"
            android:layout_marginBottom="10dp"
            android:textColor="#9FA6AD"
            android:id="@+id/tvLoaddate"
            android:text="Load Date" />
            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_gravity="center|right"
            android:textSize="@dimen/tv_text_size"
            android:textColor="@color/dark_gray"
            android:id="@+id/tvDispalyloaddate"/>
           </LinearLayout>
         </LinearLayout>
         <LinearLayout
          android:layout_width="match_parent"
          android:orientation="vertical"
          android:layout_height="wrap_content">
           <ImageView
             android:src="@drawable/icon_arrow_blue"
             android:id="@+id/imgArrowblue"
             android:layout_gravity="center"
             android:scaleType="centerInside"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_weight="1" />
         </LinearLayout>
  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_alignParentBottom="true"
      android:orientation="vertical"
      android:layout_marginBottom="20dp"
      android:layout_weight="3">
         <LinearLayout
          android:layout_width="match_parent"
          android:orientation="horizontal"
          android:layout_marginLeft="20dp"
          android:layout_marginRight="20dp"
          android:layout_height="wrap_content"
          android:background="@drawable/background_shadow"
          android:layout_weight="1">
           <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="left"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="15dp"
            android:orientation="horizontal"
            android:layout_weight="2.5">
             <ImageView
             android:src="@drawable/icon_Move_delivery"
             android:id="@+id/imgMovedelivery"
             android:layout_gravity="center"
             android:scaleType="centerInside"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"/>
           </LinearLayout>
           <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:gravity="center|right"
           android:orientation="vertical"
           android:layout_marginRight="20dp"
           android:layout_weight="0.5">
              <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_gravity="center|right"
            android:textSize="@dimen/twelve_dp"
            android:textColor="#9FA6AD"
            android:layout_marginBottom="10dp"
            android:id="@+id/tvDeliverySpread"
            android:text="Delivery Spread" />
            <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:layout_gravity="center|right"
            android:textSize="@dimen/tv_text_size"
            android:textColor="@color/dark_gray"
            android:id="@+id/tvDispalyDeliverySpread"
            />
           </LinearLayout>
         </LinearLayout>
         <LinearLayout
          android:layout_width="match_parent"
          android:orientation="vertical"
          android:layout_height="wrap_content">
           <ImageView
             android:src="@drawable/icon_arrow_gold"
             android:id="@+id/imgArrowgold"
             android:layout_gravity="center"
             android:scaleType="centerInside"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_weight="1" />
         </LinearLayout>
  </LinearLayout>
</LinearLayout>