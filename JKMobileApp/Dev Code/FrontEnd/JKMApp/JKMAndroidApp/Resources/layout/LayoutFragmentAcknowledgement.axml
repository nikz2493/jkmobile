<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7F8FD">
  <ScrollView
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="#F7F8FD"
      android:layout_above="@+id/linearLayoutBottom">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="16dp"
        android:paddingLeft="16dp"
        android:paddingBottom="16dp">
      <TextView
          android:id="@+id/textViewHeading"
          android:layout_width="match_parent"
          android:layout_height="wrap_content" 
          android:textColor="#d3d3d3"
          android:textSize="@dimen/titleDiscription_text_size"
          android:text="Please review and confirm all information is accurate." />
      <LinearLayout
          android:layout_marginTop="10dp"
          android:padding="25dp"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:background="@drawable/background_shadow"
          android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/relativeLayoutServiceDate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
          <ImageView
              android:id="@+id/imageViewServiceDate"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/checked_green"
              android:layout_centerVertical="true" />
          <TextView
              android:id="@+id/textViewServiceDate"
              android:layout_marginLeft="10dp"
              android:layout_toRightOf="@+id/imageViewServiceDate"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:textSize="@dimen/text_size_tvServicetitle"
              android:text="Service Date"
              android:textColor="@color/dark_gray"
              android:textStyle="bold"
              android:layout_centerVertical="true" />
          <ImageView
              android:layout_centerVertical="true"
              android:id="@+id/imageViewEditServiceDate"
              android:layout_alignParentRight="true"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/edit_icon_selector" />
        </RelativeLayout>
        <LinearLayout
            android:visibility="gone"
            android:id="@+id/linearLayoutServiceDateEdit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background_shadow"
            android:orientation="vertical">
          <RelativeLayout
              android:paddingRight="12dp"
              android:paddingLeft="12dp"
              android:layout_marginTop="5dp"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">
            <TextView
                android:id="@+id/textViewPackDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_tvServicetitle"
                android:text="Pack Date"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/textViewDateEditPack"
                android:layout_alignParentRight="true"
                android:layout_width="@dimen/standard_80"
                android:layout_height="@dimen/standard_25"
                android:textSize="@dimen/standard_12"
                
                android:textColor="@color/dark_gray"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/rectangle_gray" />
          </RelativeLayout>
          <View
              android:layout_marginLeft="12dp"
              android:layout_marginRight="12dp"
              android:layout_marginTop="5dp"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#E9EAEF" />
          <RelativeLayout
              android:paddingRight="12dp"
              android:paddingLeft="12dp"
              android:layout_marginTop="5dp"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">
            <TextView
                android:id="@+id/textViewLoadDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_tvServicetitle"
                android:text="Load Date"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/textViewDateEditLoad"
                android:layout_alignParentRight="true"
                android:layout_width="@dimen/standard_80"
                android:layout_height="@dimen/standard_25"
                android:textSize="@dimen/standard_12"
                android:textColor="@color/dark_gray"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/rectangle_gray" />
          </RelativeLayout>
          <View
              android:layout_marginLeft="12dp"
              android:layout_marginRight="12dp"
              android:layout_marginTop="5dp"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:background="#E9EAEF" />
          <RelativeLayout
              android:paddingRight="12dp"
              android:paddingLeft="12dp"
              android:layout_marginTop="5dp"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">
            <TextView
                android:id="@+id/textViewMoveDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/text_size_tvServicetitle"
                android:text="Delivery Date"
                android:layout_centerVertical="true" />
            <TextView
                android:id="@+id/textViewDateEditMove"
                android:layout_alignParentRight="true"
                android:layout_width="@dimen/standard_80"
                android:layout_height="@dimen/standard_25"
                android:textSize="@dimen/standard_12"
                android:textColor="@color/dark_gray"
                android:textStyle="bold"
                android:gravity="center"
                android:background="@drawable/rectangle_gray" />
          </RelativeLayout>

        </LinearLayout>
        <RelativeLayout
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
          <ImageView
              android:id="@+id/imageViewServices"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/checked_green"
              android:layout_centerVertical="true" />
          <TextView
              android:id="@+id/textViewServices"
              android:layout_marginLeft="10dp"
              android:layout_toRightOf="@+id/imageViewServices"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:textSize="@dimen/text_size_tvServicetitle"
              android:text="Services"
              android:textColor="@color/dark_gray"
              android:textStyle="bold"
              android:layout_centerVertical="true" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
          <ImageView
              android:id="@+id/imageViewVitalInformation"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/checked_green"
              android:layout_centerVertical="true" />
          <TextView
              android:id="@+id/textViewVitalInformation"
              android:layout_marginLeft="10dp"
              android:layout_toRightOf="@+id/imageViewVitalInformation"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:textSize="@dimen/text_size_tvServicetitle"
              android:text="Vital Information"
              android:textColor="@color/dark_gray"
              android:textStyle="bold"
              android:layout_centerVertical="true" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/relativeLayoutAddresses"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
          <ImageView
              android:id="@+id/imageViewAddresses"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/checked_green"
              android:layout_centerVertical="true" />
          <TextView
             android:id="@+id/textViewAddresses"
              android:layout_marginLeft="10dp"
              android:layout_toRightOf="@+id/imageViewAddresses"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:textSize="@dimen/text_size_tvServicetitle"
              android:text="Addresses"
              android:textColor="@color/dark_gray"
              android:textStyle="bold"
              android:layout_centerVertical="true" />
          <ImageView
              android:layout_centerVertical="true"
              android:id="@+id/imageViewEditAddresses"
              android:layout_alignParentRight="true"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/edit_icon_selector" />
        </RelativeLayout>
        <LinearLayout
            android:visibility="gone"
            android:id="@+id/linearLayoutAddressesEdit"
            android:paddingTop="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/background_shadow"
            android:orientation="vertical">
          <LinearLayout
              android:paddingRight="12dp"
              android:paddingLeft="12dp"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
              <TextView
                  android:id="@+id/textViewOrigin"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:textSize="@dimen/text_size_tvServicetitle"
                  android:text="Origin"
                  android:textAllCaps="true" />
              <EditText
                  android:layout_marginTop="5dp"
                  android:id="@+id/editTextAddressOrigin"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:textSize="@dimen/standard_12"
                  android:theme="@style/NormalEditTextStyle"
                  android:textColor="@color/dark_gray"
                  android:maxLines="4"
                  android:background="@null" />
            </LinearLayout>
            <View
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#E9EAEF" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
              <TextView
                  android:id="@+id/textViewDestination"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:textSize="@dimen/text_size_tvServicetitle"
                  android:text="Destination"
                  android:textAllCaps="true" />
              <EditText
                  android:layout_marginTop="5dp"
                  android:id="@+id/editTextAddressDestination"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:textSize="@dimen/standard_12"
                  android:theme="@style/NormalEditTextStyle"
                  android:textColor="@color/dark_gray"
                  android:maxLines="3"
                  android:background="@null" />
            </LinearLayout>
          </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
          <ImageView
              android:id="@+id/imageViewValuation"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/checked_green"
              android:layout_centerVertical="true" />
          <TextView
              android:id="@+id/textViewValuation"
              android:layout_marginLeft="10dp"
              android:layout_toRightOf="@+id/imageViewValuation"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:textSize="@dimen/text_size_tvServicetitle"
              android:text="Valuation"
              android:textColor="@color/dark_gray"
              android:textStyle="bold"
              android:layout_centerVertical="true" />
          <ImageView
               android:layout_centerVertical="true"
               android:id="@+id/imageViewEditValution"
               android:layout_alignParentRight="true"
               android:layout_width="@dimen/fifteen_dp"
               android:layout_height="@dimen/fifteen_dp"
               android:src="@drawable/edit_icon_selector" />
        </RelativeLayout>
        <RelativeLayout
             android:visibility="gone"
             android:id="@+id/relativeLayoutValutionEdit"
             android:layout_width="match_parent"
             android:layout_height="@dimen/standard_170"
             android:background="@drawable/background_shadow">
          <ScrollView
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_above="@+id/linearLayoutBottom">
            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
              <LinearLayout
                  android:id="@+id/linearLayoutEdit"
                  android:paddingTop="3dp"
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:orientation="vertical">
                <LinearLayout
                   android:paddingRight="16dp"
                   android:paddingLeft="16dp"
                   android:orientation="vertical"
                   android:layout_marginTop="3dp"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content">
                  <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/standard_9"
                    android:text="DECLARED VALUE"
                    android:id="@+id/tvEditDeclaredvalue"
                    android:layout_centerVertical="true" />
                  <EditText
                     android:numeric="integer"
                     android:inputType="number"
                     android:maxLength="20"
                     android:layout_width="match_parent"
                     android:layout_height="@dimen/standard_30"
                     android:id="@+id/txtDeclaredvalue"
                     android:theme="@style/NormalEditTextStyle"
                     android:background="@drawable/EditText_Border"
                     android:textSize="@dimen/text_size_tvServicetitle"
                     android:textColor="@color/dark_gray"
                     android:textStyle="bold"  />
                </LinearLayout>
                <View
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E9EAEF" />
                <LinearLayout
                     android:paddingRight="16dp"
                     android:paddingLeft="16dp"
                     android:orientation="vertical"
                     android:layout_marginTop="5dp"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content">
                  <TextView
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:textSize="@dimen/standard_9"
                       android:text="COVERAGE"
                       android:id="@+id/tvEditCoverage"
                       android:layout_centerVertical="true" />
                  <Spinner
                        android:id="@+id/spinnerCoverage"
                        android:theme="@style/NormalEditTextStyle"
                        android:background="@drawable/SpiinereBorder"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/standard_30"
                        android:textSize="@dimen/text_size_tvServicetitle"
                        android:textColor="@color/dark_gray"
                        android:textStyle="bold"/>
                </LinearLayout>
                <View
                   android:layout_marginLeft="16dp"
                   android:layout_marginRight="16dp"
                   android:layout_marginTop="5dp"
                   android:layout_width="match_parent"
                   android:layout_height="1dp"
                   android:background="#E9EAEF" />
                <LinearLayout
                        android:paddingRight="16dp"
                        android:paddingLeft="16dp"
                        android:orientation="vertical"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:textSize="@dimen/standard_9"
                      android:text="COST"
                      android:id="@+id/tvEditCost"
                      android:layout_centerVertical="true" />
                  <EditText
                      android:layout_width="match_parent"
                      android:layout_height="@dimen/standard_30"
                      android:id="@+id/txtCost"
                      android:numeric="integer"
                      android:inputType="number"
                      android:editable="false"
                      android:clickable="false"
                      android:cursorVisible="false"
                      android:focusable="false"
                      android:textSelectHandle="@drawable/empty_cursor"
                      android:textSelectHandleLeft="@drawable/empty_cursor"
                      android:textSelectHandleRight="@drawable/empty_cursor"
                      android:focusableInTouchMode="false"
                      android:theme="@style/NormalEditTextStyle"
                      android:background="@drawable/EditText_Border"
                      android:textSize="@dimen/text_size_tvServicetitle"
                      android:textColor="@color/dark_gray"
                      android:textStyle="bold"
                      android:gravity="center_vertical"/>
                </LinearLayout>
              </LinearLayout>
            </LinearLayout>
          </ScrollView>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/relativeLayoutWhatMattersMost"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
          <ImageView
              android:id="@+id/imageViewWhatMattersMost"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/checked_green"
              android:layout_centerVertical="true" />
          <TextView
              android:id="@+id/textViewWhatMattersMost"
              android:layout_marginLeft="10dp"
              android:layout_toRightOf="@+id/imageViewWhatMattersMost"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:textSize="@dimen/text_size_tvServicetitle"
              android:text="What Matters Most"
              android:textColor="@color/dark_gray"
              android:textStyle="bold"
              android:layout_centerVertical="true" />
          <ImageView
              android:layout_centerVertical="true"
              android:id="@+id/imageViewEditWhatMattersMost"
              android:layout_alignParentRight="true"
              android:layout_width="@dimen/fifteen_dp"
              android:layout_height="@dimen/fifteen_dp"
              android:src="@drawable/edit_icon_selector" />
        </RelativeLayout>
        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/relativeLayoutWhatMattersMostEdit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/standard_140"
            android:background="@drawable/background_shadow">
          <LinearLayout
              android:layout_width="match_parent"
              android:layout_height="@dimen/standard_110"
              android:orientation="vertical"
              android:gravity="center">
            <EditText
                android:id="@+id/editTextWhatMattersMost"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/text_size_tvServicetitle"
                android:textColor="@color/dark_gray"
                android:textStyle="bold"
                android:scrollbars="vertical"
                android:theme="@style/NormalEditTextStyle"
                android:background="@drawable/rectangle_gray"
                android:layout_marginBottom="@dimen/standard_30"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"/>
          </LinearLayout>

        </RelativeLayout>
      </LinearLayout>
      <LinearLayout
          android:layout_marginTop="10dp"
          android:padding="16dp"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:background="@drawable/background_shadow"
          android:orientation="vertical">
        <TextView
          android:id="@+id/textViewCheckBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/text_size_tvServicetitle"
            android:text="By selecting, I agree, you are confirming receipt and acknowledgement of all required move related documentation." />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="I agree"
            android:theme="@style/NormalEditTextStyle"
            android:id="@+id/agreeCheckBox"
            android:textSize="@dimen/text_size_tvServicetitle" />
      </LinearLayout>
    </LinearLayout>
  </ScrollView>
  <LinearLayout
      android:id="@+id/linearLayoutBottom"
      android:layout_alignParentBottom="true"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      android:gravity="center_horizontal">
    <LinearLayout
       android:layout_width="@dimen/standard_150"
        android:layout_height="@dimen/standard_50"
        android:id="@+id/linearLayoutViewEstimate"
        android:background="@drawable/rounded_background_shadow"
        android:orientation="horizontal"
        android:gravity="center">
      <ImageView
          android:layout_width="@dimen/eighteen_sp"
          android:layout_height="@dimen/eighteen_sp"
          android:src="@drawable/pdf" />
      <TextView
          android:id="@+id/textViewViewEstimate"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="View Estimate"
          android:textSize="@dimen/tv_text_size"
          android:layout_marginLeft="10dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="25dp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/standard_50"
        android:orientation="horizontal">
      <LinearLayout
       android:id="@+id/textViewBack"
       android:layout_weight="1"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:text="Back"
       android:gravity="center"
       android:background="#69789A"
        >
        <ImageButton
                     android:src="@drawable/arrow_back"
                     android:id="@+id/btnBack"
                     android:layout_gravity="left"
                     android:scaleType="centerInside"
                     android:background="#69789A"
                     android:layout_width="@dimen/standard_170"
                     android:layout_weight="1"
                     android:layout_height="@dimen/standard_48" />
        <TextView
                   android:id="@+id/tvback"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:gravity="left"
                   android:textSize="@dimen/btn_text_size"
                   android:layout_weight="1"
                   android:textColor="@android:color/white"
                   android:text="Back" />
      </LinearLayout>
      <TextView
          android:layout_weight="1"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:id="@+id/textViewNext"
          android:text="Continue"
          android:gravity="center"
          android:textSize="@dimen/btn_text_size"
          android:background="#EC3D62"
          android:textColor="@android:color/white" />
    </LinearLayout>
  </LinearLayout>
</RelativeLayout>